{
	"name": "kubedraw-server",
	"version": "1.0.0",
	"main": "main.js",
	"types": "main.ts",
	"license": "Apache-2.0",
	"private": true,
	"scripts": {
		"nodemon": "node node_modules/nodemon/bin/nodemon.js --exec node_modules/ts-node/dist/bin.js main.ts",
		"import": "cdk8s import",
		"synth": "cdk8s synth",
		"compile": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"build": "npm run compile && npm run test && npm run synth",
		"upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
		"upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
	},
	"dependencies": {
		"@types/js-yaml": "^3.12.5",
		"cdk8s": "^0.25.0",
		"constructs": "^2.0.2",
		"express": "^4.17.1",
		"express-graphql": "^0.9.0",
		"graphql": "^15.1.0",
		"js-yaml": "^3.14.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/jest": "^26.0.3",
		"@types/node": "^14.0.14",
		"cdk8s-cli": "^0.25.0",
		"jest": "^26.1.0",
		"nodemon": "^2.0.4",
		"ts-jest": "^26.1.1",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.5"
	}
}
